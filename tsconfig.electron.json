{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "outDir": "dist-electron",
      "rootDir": "electron",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noEmitOnError": true
    },
    "include": ["electron"]
  }
  